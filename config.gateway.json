{
  "firewall": {
    "source-validation": "disable"
  },
  "interfaces": {
    "ethernet": {
      "eth0": {
        "description": "WAN",
        "duplex": "auto",
        "speed": "auto",
        "vif": {
          "34": {
            "description": "eth0.34 - Internet",
            "address": [
              "dhcp"
            ],
            "dhcp-options": {
              "default-route-distance": "1",
                "default-route": "update",
                "name-server": "update"
            },
            "firewall": {
              "in": {
                "name": "WAN_IN"
              },
              "local": {
                "name": "WAN_LOCAL"
              },
              "out": {
                "name": "WAN_OUT"
              }
            }
          },
          "4": {
            "address": [
              "dhcp"
            ],
            "mac": "B4:Fb:E4:81:E2:EE",
    	    "description": "eth0.4 - IPTV",
            "dhcp-options": {
              "client-option": [
                "send vendor-class-identifier &quot;IPTV_RG&quot;;",
                "request subnet-mask, routers, rfc3442-classless-static-routes;"
              ],
              "default-route": "no-update",
              "default-route-distance": "210"
            },
            "ip": {
              "source-validation": "loose"
            }
          }
        }
      }
    }
  },
  "port-forward": {
    "auto-firewall": "enable",
    "wan-interface": "eth0.34"
  },
  "protocols": {
    "igmp-proxy": {
      "interface": {
        "eth2": {
          "role": "downstream",
          "threshold": "1",
          "whitelist": [
            "0.0.0.0/0"
          ]
        },
	"eth1": {
          "role": "disabled",
          "threshold": "1"
        },
        "eth0": {
          "role": "disabled",
          "threshold": "1"
        },
        "eth0.4": {
          "alt-subnet": [
            "0.0.0.0/0"
          ],
          "role": "upstream",
          "threshold": "1"
        },
        "eth0.34": {
          "role": "disabled",
          "threshold": "1"
        }
      }
    }
  },
  "service": {
    "dns": {
      "forwarding": {
        "except-interface": [
          "eth0",
          "eth0.34",
          "eth0.4"
        ]
      }
    },
    "nat": {
      "rule": {
        "2": {
          "description": "ipv-srtp-abox-1",
          "inbound-interface": "eth0.4",
          "inside-address": {
            "address": "10.0.1.10",
            "port": "555-65000"
          },
          "log": "disable",
          "protocol": "udp",
          "source": {
            "address": "185.41.48.0/24"
          },
          "type": "destination"
        },
        "3": {
          "description": "ipv-srtp-abox-2",
          "inbound-interface": "eth0.4",
          "inside-address": {
            "address": "10.0.1.11",
            "port": "555-65000"
          },
          "log": "disable",
          "protocol": "udp",
          "source": {
            "address": "185.41.48.0/24"
          },
          "type": "destination"
        },
        "5000": {
          "description": "MASQ corporate_network to IPTV network",
          "log": "disable",
          "outbound-interface": "eth0.4",
          "protocol": "all",
          "type": "masquerade"
        },
        "5001": {
          "description": "MASQ corporate_network to WAN",
          "log": "disable",
          "outbound-interface": "eth0.34",
          "protocol": "all",
          "type": "masquerade"
        }
      }
    }
  },
  "system": {
    "conntrack": {
      "modules": {
        "sip": "disable"
      },
      "expect-table-size": "2048",
      "hash-size": "32768",
      "table-size": "262144"
    }
  }
}
